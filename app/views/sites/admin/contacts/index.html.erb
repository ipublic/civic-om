<div class="row">
	<div class="twelve columns">
	  <p><%= link_to 'Add Contact', new_admin_contact_path %> </p>
	  <hr />
	</div>
</div>
<div class="row">
	<div class="twelve columns">
		<h2>Contacts</h2>
